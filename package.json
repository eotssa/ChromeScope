        "lodash": "^4.17.21",
        "mongoose": "^8.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@flydotio/dockerfile": "^0.4.10",
        "cross-env": "^7.0.3",
        "eslint": "^8.53.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    }
